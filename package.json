    "intersection-observer": "0.12.0",
    "next": "^12.3.4",
    "next-sanity": "0.4.0",
    "prismjs": "^1.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-in-viewport": "1.0.0-alpha.28",
    "react-social-icons": "^5.15.0",
