{
  "name": "detsombetyrnoe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next\" \"cd sanity && sanity start\"",
    "build": "echo 'Building Sanity to public/studioâ€¦' && cd sanity && sanity build ../public/studio -y && cd .. && next build",
    "bump": "npx npm-check-updates --target minor -u && npm i",
    "start": "next start",
    "test": "jest --watch",
    "test-ci": "jest",
    "typeCheck": "tsc",
    "postinstall": "lerna bootstrap",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i"
  },
  "dependencies": {
    "@apollo/client": "3.3.15",
    "@sanity/block-content-to-react": "2.0.7",
    "@sentry/browser": "6.3.0",
    "@u-wave/react-vimeo": "0.9.4",
    "amplitude-js": "8.1.0",
    "axios": "0.21.1",
    "basicscroll": "3.0.4",
    "date-fns": "2.21.1",
    "framer-motion": "3.10.6",
    "graphql": "15.5.0",
    "identity-obj-proxy": "3.0.0",
    "intersection-observer": "0.12.0",
    "next": "11.1.0",
    "next-sanity": "0.1.12",
    "prismjs": "1.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-in-viewport": "1.0.0-alpha.16",
    "react-use": "17.2.3",
    "striptags": "3.2.0",
    "styled-components": "5.2.3",
    "swr": "0.5.5"
  },
  "devDependencies": {
    "@sanity/cli": "2.8.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.5",
    "@types/amplitude-js": "7.0.1",
    "@types/basicscroll": "3.0.0",
    "@types/node": "14.14.41",
    "@types/prismjs": "1.16.5",
    "@types/react": "17.0.3",
    "@types/styled-components": "5.1.9",
    "babel-jest": "26.6.3",
    "babel-plugin-macros": "3.0.1",
    "babel-plugin-styled-components": "1.12.0",
    "concurrently": "^6.0.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  },
  "prettier": {
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run typeCheck && npm run test-ci"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  }
}
