{
  "name": "detsombetyrnoe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next\" \"cd sanity && sanity start\"",
    "next-dev": "next",
    "build": "echo 'Building Sanity to public/studioâ€¦' && cd sanity && sanity build ../public/studio -y && cd .. && next build",
    "bump": "npx npm-check-updates --target minor -u && npm i",
