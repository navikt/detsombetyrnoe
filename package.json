    "@types/styled-components": "^5.1.26",
    "concurrently": "6.0.2",
    "husky": "4.3.8",
    "jest": "27.4.3",
    "lerna": "4.0.0",
    "lint-staged": "10.5.4",
    "prettier": "^2.8.4",
    "react-test-renderer": "17.0.2",
    "styled-components": "^5.3.6",
    "typescript": "4.4.2"
